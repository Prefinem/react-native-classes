{
	"author": "William <william@prefinem.com>",
	"description": "Bringing CSS Classes to React Native",
	"devDependencies": {
		"@babel/cli": "^7.1.0",
		"@babel/core": "^7.1.2",
		"babel-eslint": "^9.0.0",
		"babel-preset-expo": "^5.0.0",
		"eslint": "^5.6.1",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-babel": "^5.2.1",
		"eslint-plugin-react": "^7.11.1",
		"husky": "^1.1.3",
		"lint-staged": "^7.3.0",
		"prettier": "^1.14.3"
	},
	"files": [
		"src/"
	],
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"git add"
		]
	},
	"main": "src/index.js",
	"name": "react-native-classes",
	"private": false,
	"repository": "https://github.com/Prefinem/react-native-classes",
	"scripts": {
		"clean": "yarn lint && yarn prettify",
		"lint": "eslint",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"reset": "knex migrate:rollback && knex migrate:latest && node import/sources.js",
		"staged": "lint-staged",
		"start": "node ./index.js"
	},
	"version": "0.1.0"
}
